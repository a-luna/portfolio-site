{
	"name": "portfolio",
	"version": "0.0.1",
	"engines": {
		"node": "18.11.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite --host --port 3504",
		"start": "vite preview --host",
		"dokku": "npm install",
		"build": "svelte-kit sync && vite build && ln -s -f build/index.js server.js",
		"preview": "vite preview --host",
		"sync": "svelte-kit sync && pnpm-lock-export",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"up": "pnpm update && npm i --package-lock-only"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/typography": "^0.5.7",
		"date-fns": "^2.29.3",
		"gray-matter": "^4.0.3",
		"highlight.js": "^11.6.0",
		"js-base64": "^3.7.2",
		"mdsvex": "^0.10.6",
		"node-fetch": "2.6.7",
		"parse-link-header": "^2.0.0",
		"path-browserify": "^1.0.1",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.0.1",
		"rehype-stringify": "^9.0.3",
		"remark-abbr": "^1.4.1",
		"remark-frontmatter": "^4.0.1",
		"remark-github": "^11.2.4",
		"remark-parse": "^10.0.1",
		"remark-sectionize": "^1.1.1",
		"rss": "^1.2.2",
		"shiki": "^0.10.1",
		"slugify": "^1.6.5",
		"snarkdown": "^2.0.0",
		"svelte": "^3.52.0",
		"svelte-highlight": "^6.2.1",
		"tailwindcss": "^3.2.1",
		"to-vfile": "^7.2.3",
		"unified": "^10.1.2",
		"vfile": "^5.3.5",
		"vfile-reporter": "^7.0.4",
		"vite": "^3.2.1"
	},
	"devDependencies": {
		"@pnpm/logger": "^2.1.1",
		"@stefanprobst/remark-shiki": "^2.1.0",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"node-html-parser": "^6.1.1",
		"pnpm-lock-export": "^0.4.0",
		"postcss": "^8.4.18",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"rehype-document": "^6.1.0",
		"rehype-format": "^4.0.1",
		"rehype-parse": "^8.0.4",
		"rehype-raw": "^6.1.1",
		"remark-gfm": "^3.0.1",
		"remark-rehype": "^10.1.0",
		"remark-toc": "^8.0.1",
		"svelte-check": "^2.9.2",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.8.4",
		"vite-imagetools": "^4.0.11"
	},
	"type": "module"
}
